{
  "name": "@decoupla/sdk",
  "version": "0.1.4",
  "description": "Type-safe TypeScript client for the Decoupla content management API",
  "author": "Decoupla",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Decoupla/decoupla-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Decoupla/decoupla-sdk/issues"
  },
  "homepage": "https://decoupla.com",
  "license": "MIT",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    }
  },
  "bin": {
    "decoupla": "bin/decoupla.cjs"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "setup": "bun tests/setup.ts",
    "test": "bun test --timeout 30000",
    "test:types": "tsd",
    "test:unit": "bun test tests/unit --timeout 30000",
    "test:integration": "bun test tests/integration --timeout 30000",
    "test:watch": "bun test --watch --timeout 30000",
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "tsup",
    "build": "npm run build:types && npm run build:js",
    "build:cli": "tsup src/cli/index.ts --format cjs,esm --dts --out-dir dist/cli --target node14"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsd": "^0.27.0",
    "tsup": "^6.6.0"
  },
  "optionalDependencies": {
    "esbuild-register": "^3.4.0",
    "esbuild": "^0.18.0"
  },
  "dependencies": {
    "zod": "^4.1.13",
    "typescript": "^5.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "cms",
    "content-management",
    "api",
    "typescript",
    "type-safe",
    "client"
  ]
}